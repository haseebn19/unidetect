    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.6",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
